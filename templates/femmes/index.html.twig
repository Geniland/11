{% extends 'base.html.twig' %}

{% block title %}Femmes index{% endblock %}

{% block body %}
    <h1>Femmes index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>FemmeImage</th>
                <th>FemmeNom</th>
                <th>FemmePrix</th>
                <th>FemmeEtat</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for femme in femmes %}
            <tr>
                <td>{{ femme.id }}</td>
                <td>{{ femme.FemmeImage }}</td>
                <td>{{ femme.FemmeNom }}</td>
                <td>{{ femme.FemmePrix }}</td>
                <td>{{ femme.FemmeEtat }}</td>
                <td>
                    <a href="{{ path('app_femmes_show', {'id': femme.id}) }}">show</a>
                    <a href="{{ path('app_femmes_edit', {'id': femme.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_femmes_new') }}">Create new</a>
{% endblock %}
